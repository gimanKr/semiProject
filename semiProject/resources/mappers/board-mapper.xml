<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="boardMapper">
  	<resultMap id ="boardResultSet" type="Board">
  		<result column="EXBOARD_NO" property="exboardNo"/>
  		<result column="USER_NO" property="userNo"/>
  		<result column="EXBOARD_TITLE" property="exboardTitle"/>
  		<result column="EXBOARD_TEXT" property="exboardText"/>
  		<result column="EXBOARD_CATEGORY" property="exboardCategory"/>
  		<result column="UPDATE_DATE" property="updateDate"/>
  		<result column="DELETE_DATE" property="deleteDate"/>
  		<result column="BOARD_COUNT" property="boardCount"/>
  	</resultMap>
  
	  <resultMap id="CommentResultSet" type="Reply">
	      <result column="EXBOARD_COMMENT_NO" property="exboardCommentNo" />
	      <result column="EXBOARD_COMMENT_TEXT" property="exboardCommentText" />
	      <result column="EXBOARD_NO" property="exboardNo" />
	      <result column="USER_NO" property="userNo" />
	   </resultMap>
	   
	   
	
  
  
  
  </mapper>